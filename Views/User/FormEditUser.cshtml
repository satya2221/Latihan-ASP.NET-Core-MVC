@model WebMVC_DAL.Models.Dto.Res.ResUserDto
@{
    ViewData["Title"] = "Edit User";
}
<div>
    <h1>Add User</h1>
    <form method="post" asp-controller="User" asp-action="EditUser" class="mt-5">
        <input type="hidden" asp-for="Id"  value="@Model.Id"/>
        <div class="mb-3">
            <label class="form-label">User Name</label>
            <input type="text" class="form-control" asp-for="UserName" value="@Model.UserName"/>
        </div>
        <div class="mb-3">
            <label class="form-label">Pekerjaan</label>
            <input type="text" class="form-control" asp-for="Pekerjaan" value="@Model.Pekerjaan"/>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
